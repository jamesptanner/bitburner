{
    "compilerOptions": {
        "module": "esnext",
        "target": "esnext",
        "moduleResolution": "node",
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "esModuleInterop": true,
        "inlineSourceMap": true,
        "inlineSources": true,
        "strict": true,
        "rootDir": "src/",
        "outDir": "dist/",
        "baseUrl": "src/",
        "paths": {
          "/*": ["*"],
          "@ns": ["../NetScriptDefinitions.d.ts"],
          "@react": ["lib/react.ts"]
        }
    },
    "include": ["NetScriptDefinitions.d.ts","src/**/*"],
    "exclude": ["src/lib/react.ts"],
    "typedocOptions":{
      "entryPoints": ["NetScriptDefinitions.d.ts"],
      "out": "docs"
    }


}